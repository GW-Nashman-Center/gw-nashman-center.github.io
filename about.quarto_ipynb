        "})\n",
        "\n",
        "# Create a horizontal bar chart\n",
        "fig = px.bar(impact_data, \n",
        "             y='Metric', \n",
        "             x='Value', \n",
        "             orientation='h',\n",
        "             title='Our Impact Since 2020',\n",
        "             color='Value',\n",
        "             color_continuous_scale='Blues')\n",
        "\n",
        "fig.update_layout(\n",
        "    height=300,\n",
        "    showlegend=False,\n",
        "    xaxis_title='',\n",
