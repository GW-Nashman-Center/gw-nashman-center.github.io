        "#| echo: false\n",
        "import pandas as pd\n",
        "import plotly.express as px\n",
        "\n",
        "# Impact metrics\n",
        "impact_data = pd.DataFrame({\n",
        "    'Metric': ['Clients Served', 'Projects Completed', 'Team Members', 'Years of Experience'],\n",
